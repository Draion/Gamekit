ALIASES += LuaSectionUp{1}=""
ALIASES += LuaIndexUp{1}=""
ALIASES += LuaClassUp{1}=""


ALIASES += LuaClass{1}="\class \1"
ALIASES += LuaClassExtend{1}="\implements \1"
ALIASES += LuaMethod{2}="\section \1_\2 \1.\2"
ALIASES += LuaIsA{2}="<hr>\b \1: Is a type of \LuaClassRef{\2}"
ALIASES += LuaExtern{2}="\class \1"


ALIASES += LuaSee{2}="\see \ref \1_\2"
ALIASES += LuaClassRef{1}="\ref \1"
ALIASES += LuaMethodReturn{2}="\returns \ref \1 \2"
ALIASES += LuaGlobal{1}="\page \1  \1 "
ALIASES += LuaGlobal{2}="\page \1  \2 "
ALIASES += LuaGlobalRef{1}="\see \ref \1"
ALIASES += LuaExample{1}="\section \1"
ALIASES += LuaExample{2}="\section \1 \2"


ALIASES += beginmenu="<hr><table class=\"nav\" cellspacing=\"0\" cellpadding=\"0\">"
ALIASES += beginmenu{1}="<hr><b>\1</b><table class=\"nav\" cellspacing=\"0\" cellpadding=\"0\">"
ALIASES += beginmenu{2}="<hr><b>\1</b><p>\2</p><table class=\"nav\" cellspacing=\"0\" cellpadding=\"0\">"
ALIASES += menu{1}="<tr><td class=\"left\">\ref \1</td><td></td></tr>"
ALIASES += menu{2}="<tr><td class=\"left\">\ref \1</td><td class=\"right\"><i>\2</i></td></tr>"
ALIASES += submenu{1}="<tr><td class=\"left\">\ref \1</td><td></td></tr>"
ALIASES += submenu{2}="<tr><td class=\"left\">\ref \1</td><td class=\"right\"><i>\2</i></td></tr>"
ALIASES += LuaClassMenu{1}="<tr><td class=\"left\">\ref \1</td><td></td></tr>"
ALIASES += LuaClassMenu{2}="<tr><td class=\"left\">\ref \1</td><td><i>\2</i></td></tr>"
ALIASES += LuaGlobalMenu{1}="<tr><td class=\"left\">\ref  \1</td><td></td></tr>"
ALIASES += LuaGlobalMenu{2}="<tr><td class=\"left\">\ref  \1</td><td><i>\2</i></td></tr>"
ALIASES += LuaMethodMenu{2}="<tr><td class=\"left\">\ref \1_\2</td><td></td></tr>"
ALIASES += endmenu="</table><br>\n\n"

ALIASES += endpage="<br>\n\n<hr>\n\n"
ALIASES += sectionseperator="\n\n"
ALIASES += sectionseperator{1}="\n\n<b>\1</b>\n\n"

